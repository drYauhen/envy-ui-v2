{
  "name": "envy-ui-v2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "check:zone": "find . -name '*:Zone.Identifier'",
    "clean:zone": "find . -name '*:Zone.Identifier' -delete",
    "clean:repo": "npm run clean:zone",
    "tokens:build": "style-dictionary build --config style-dictionary/config.mjs",
    "tokens:build:tokenstudio": "style-dictionary build --config style-dictionary/config.mjs --platform tokenstudio",
    "tokens:watch": "nodemon --watch tokens --ext json --exec \"npm run tokens:build\"",
    "docs:copy": "node scripts/copy-docs-to-public.mjs",
    "docs:watch": "nodemon --watch docs --ext md --exec \"npm run docs:copy\"",
    "tokens:copy-readme": "node scripts/copy-tokens-readme.mjs",
    "storybook:sync-nav": "node scripts/sync-navigation-order.mjs",
    "storybook:sync-config": "node scripts/sync-navigation-config.mjs",
    "tailwind:generate-config": "node scripts/generate-tailwind-config.mjs",
    "tailwind:watch": "nodemon --watch tokens --ext json --exec \"npm run tailwind:generate-config\"",
    "storybook:prepare": "npm run tokens:build && npm run docs:copy && npm run tokens:copy-readme && npm run tailwind:generate-config",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:restart": "npm run docs:copy && npm run storybook:dev",
    "storybook": "npm run storybook:prepare && nodemon",
    "build-storybook": "npm run storybook:build",
    "storybook:build": "npm run tokens:build && npm run docs:copy && npm run tokens:copy-readme && storybook build",
    "figma:build": "esbuild figma-plugin/code.ts --bundle --format=iife --platform=browser --target=es2017 --outfile=figma-plugin/dist/code.js --loader:.html=text --loader:.json=json",
    "figma:analyze": "node scripts/figma/analyze-snapshot.mjs",
    "figma:migration:generate": "node scripts/figma/generate-migration.mjs",
    "tokens:build:figma": "npm run tokens:build:figma:app && npm run tokens:build:figma:website && npm run tokens:build:figma:report",
    "tokens:build:figma:app": "style-dictionary build --config style-dictionary/config.mjs --platform figmaApp",
    "tokens:build:figma:website": "style-dictionary build --config style-dictionary/config.mjs --platform figmaWebsite",
    "tokens:build:figma:report": "style-dictionary build --config style-dictionary/config.mjs --platform figmaReport",
    "dev:app": "cd apps/dev-app && npm run dev",
    "build:app": "cd apps/dev-app && npm run build",
    "preview:app": "cd apps/dev-app && npm run preview",
    "icons:normalize": "node scripts/normalize-svg-icons.mjs",
    "icons:generate": "node scripts/generate-icons.mjs",
    "icons:process": "npm run icons:normalize && npm run icons:generate",
    "adr:validate": "node scripts/validate-adr.mjs",
    "adr:generate": "node scripts/generate-adr-stories.mjs",
    "docs:validate": "node scripts/validate-docs-links.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=18.17.0"
  },
  "devDependencies": {
    "@figma/code-connect": "^1.3.12",
    "@figma/plugin-typings": "^1.121.0",
    "@storybook/addon-a11y": "^10.1.9",
    "@storybook/addon-docs": "^10.1.9",
    "@storybook/react-vite": "^10.1.9",
    "@svgr/core": "^8.1.0",
    "@tanstack/react-query": "^5.90.12",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.16",
    "cheerio": "^1.1.2",
    "concurrently": "^9.2.1",
    "culori": "^4.0.2",
    "esbuild": "^0.27.1",
    "ignore": "^7.0.5",
    "mermaid": "^11.12.2",
    "msw": "^2.12.4",
    "nodemon": "^3.1.11",
    "postcss": "^8.4.32",
    "prism-react-renderer": "^2.4.1",
    "storybook": "^10.1.9",
    "style-dictionary": "^5.1.1",
    "svg-path-parser": "^1.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite-plugin-svgr": "^4.5.0"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "react": "^19.2.3",
    "react-aria": "^3.45.0",
    "react-device-frame": "^2.1.4",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-stately": "^3.43.0",
    "remark-gfm": "^4.0.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
