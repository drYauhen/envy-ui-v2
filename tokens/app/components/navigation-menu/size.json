{
  "eui": {
    "navigation-menu": {
      "size": {
        "width": {
          "expanded": {
            "$value": "300px",
            "$type": "dimension",
            "$description": "Navigation menu width when expanded. Shows icons and labels."
          },
          "collapsed": {
            "$value": "50px",
            "$type": "dimension",
            "$description": "Navigation menu width when collapsed. Shows icons only."
          }
        },
        "item": {
          "height": {
            "$value": "40px",
            "$type": "dimension",
            "$description": "Height of menu item buttons."
          },
          "title-height": {
            "$value": "50px",
            "$type": "dimension",
            "$description": "Height of title/header items (e.g., logo area)."
          },
          "toolbar-height": {
            "$value": "55px",
            "$type": "dimension",
            "$description": "Height of toolbar items at the bottom."
          },
          "section-title-height": {
            "$value": "40px",
            "$type": "dimension",
            "$description": "Height of section title elements."
          }
        },
        "icon": {
          "width": {
            "$value": "25px",
            "$type": "dimension",
            "$description": "Width of menu item icons."
          },
          "height": {
            "$value": "25px",
            "$type": "dimension",
            "$description": "Height of menu item icons."
          }
        },
        "collapser": {
          "width": {
            "$value": "20px",
            "$type": "dimension",
            "$description": "Width of collapser button."
          },
          "height": {
            "$value": "20px",
            "$type": "dimension",
            "$description": "Height of collapser button."
          },
          "icon": {
            "size": {
              "$value": "7px",
              "$type": "dimension",
              "$description": "Size of collapser icon (arrow)."
            },
            "width": {
              "$value": "1px",
              "$type": "dimension",
              "$description": "Border width of collapser icon."
            }
          }
        }
      }
    }
  }
}
