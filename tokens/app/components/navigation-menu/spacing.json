{
  "eui": {
    "navigation-menu": {
      "spacing": {
        "padding": {
          "$value": "0",
          "$type": "dimension",
          "$description": "Padding for navigation menu container (typically 0, padding handled by items)."
        },
        "item": {
          "padding": {
            "horizontal": {
              "$value": "1rem",
              "$type": "dimension",
              "$description": "Horizontal padding for menu items (left/right)."
            },
            "vertical": {
              "$value": "0",
              "$type": "dimension",
              "$description": "Vertical padding for menu items (handled by height)."
            }
          },
          "gap": {
            "$value": "0.625rem",
            "$type": "dimension",
            "$description": "Gap between icon and text in menu items."
          }
        },
        "section-title": {
          "padding": {
            "horizontal": {
              "$value": "1rem",
              "$type": "dimension",
              "$description": "Horizontal padding for section titles."
            }
          }
        },
        "separator": {
          "margin": {
            "vertical": {
              "$value": "0.5rem",
              "$type": "dimension",
              "$description": "Vertical margin for separators between menu sections."
            }
          }
        },
        "collapser": {
          "offset": {
            "right": {
              "$value": "5px",
              "$type": "dimension",
              "$description": "Right offset for collapser button when expanded."
            },
            "top": {
              "$value": "40px",
              "$type": "dimension",
              "$description": "Top offset for collapser button."
            },
            "collapsed-right": {
              "$value": "-5px",
              "$type": "dimension",
              "$description": "Right offset for collapser button when collapsed."
            }
          }
        }
      }
    }
  }
}
