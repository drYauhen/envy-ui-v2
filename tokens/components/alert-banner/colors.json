{
  "$schema": "https://schemas.s1seven.com/style-dictionary/v3/schema.json",
  "eui": {
    "alert-banner": {
      "colors": {
        "background": {
          "info": {
            "$value": "oklch(65% 0.18 240 / 0.1)",
            "$type": "color",
            "$description": "Background color for info variant (light tint with 10% opacity)"
          },
          "warning": {
            "$value": "oklch(75% 0.15 75 / 0.15)",
            "$type": "color",
            "$description": "Background color for warning variant (light tint with 15% opacity)"
          },
          "error": {
            "$value": "oklch(60% 0.22 25 / 0.1)",
            "$type": "color",
            "$description": "Background color for error variant (light tint with 10% opacity)"
          },
          "success": {
            "$value": "oklch(65% 0.20 145 / 0.1)",
            "$type": "color",
            "$description": "Background color for success variant (light tint with 10% opacity)"
          }
        },
        "border": {
          "info": {
            "$value": "{eui.color.status.info}",
            "$type": "color",
            "$description": "Border color for info variant"
          },
          "warning": {
            "$value": "{eui.color.status.warning}",
            "$type": "color",
            "$description": "Border color for warning variant"
          },
          "error": {
            "$value": "{eui.color.status.error}",
            "$type": "color",
            "$description": "Border color for error variant"
          },
          "success": {
            "$value": "{eui.color.status.success}",
            "$type": "color",
            "$description": "Border color for success variant"
          }
        },
        "text": {
          "info": {
            "$value": "{eui.color.status.info}",
            "$type": "color",
            "$description": "Text color for info variant"
          },
          "warning": {
            "$value": "{eui.color.status.warning}",
            "$type": "color",
            "$description": "Text color for warning variant"
          },
          "error": {
            "$value": "{eui.color.status.error}",
            "$type": "color",
            "$description": "Text color for error variant"
          },
          "success": {
            "$value": "{eui.color.status.success}",
            "$type": "color",
            "$description": "Text color for success variant"
          }
        }
      }
    }
  }
}
