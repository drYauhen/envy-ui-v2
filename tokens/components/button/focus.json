{
  "eui": {
    "button": {
      "focus": {
        "ring": {
          "color": {
            "derived": {
              "$value": "{eui.color.brand.primary}",
              "$type": "color"
            }
          },
          "width": {
            "base": {
              "$value": "{eui.focus.ring.width.base}",
              "$type": "dimension"
            },
            "accessible": {
              "$value": "{eui.focus.ring.width.accessible}",
              "$type": "dimension"
            }
          },
          "offset": {
            "base": {
              "$value": "0px",
              "$type": "dimension"
            }
          }
        }
      }
    }
  }
}
