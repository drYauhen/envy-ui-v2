                    "bottomRight": { "$value": "{ui.button.shape.default.radius}", "$type": "dimension" },
                    "bottomLeft": { "$value": "0px", "$type": "dimension" }
                  }
                },
                "vertical": {
                  "corner": {
                    "topLeft": { "$value": "0px", "$type": "dimension" },
                    "topRight": { "$value": "0px", "$type": "dimension" },
                    "bottomRight": { "$value": "{ui.button.shape.default.radius}", "$type": "dimension" },
                    "bottomLeft": { "$value": "{ui.button.shape.default.radius}", "$type": "dimension" }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
